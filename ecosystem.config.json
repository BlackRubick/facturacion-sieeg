{
  "name": "facturacion-sieeg",
  "script": "server-proxy.js",
  "instances": 1,
  "exec_mode": "cluster",
  "env": {
    "NODE_ENV": "production",
    "PORT": 80
  },
  "log_file": "./logs/app.log",
  "out_file": "./logs/out.log",
  "error_file": "./logs/error.log",
  "log_date_format": "YYYY-MM-DD HH:mm:ss Z",
  "restart_delay": 4000,
  "max_restarts": 10
}
